<?xml version="1.0" encoding="UTF-8"?>
<web-app >
				<!-- Register -->
	<servlet>
		<servlet-name>Registration</servlet-name>
		<servlet-class>Details.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registration</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
				<!-- Log in -->
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>Details.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
</web-app>